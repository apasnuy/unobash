git() {
     if [ "$1" = "add" -o "$1" = "stage" ]; then
         if [ "$2" = "." ]; then
             printf "'git %s .' is currently disabled by your Git wrapper.\n" "$1";
         else
             command git "$@";
         fi
     else
         command git "$@";
     fi;
 }

